<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" style="-fx-border-color: #8a8a8a; -fx-border-width: 4; -fx-border-radius: 5; -fx-background-radius: 5;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OfertController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
         <children>
            <Text fx:id="nameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" textAlignment="JUSTIFY">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
            <Text fx:id="businessmanText" strokeType="OUTSIDE" strokeWidth="0.0" text="Businessman Name">
               <VBox.margin>
                  <Insets bottom="7.0" />
               </VBox.margin>
            </Text>
            <Text fx:id="salaryText" strokeType="OUTSIDE" strokeWidth="0.0" text="Salary" />
            <Text fx:id="ubicationText" strokeType="OUTSIDE" strokeWidth="0.0" text="Ubication" />
         </children>
         <padding>
            <Insets bottom="4.0" left="5.0" right="10.0" top="4.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
